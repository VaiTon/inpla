// Fibonacci number on unary natural numbers
fib(r)><Z => r~Z;
fib(r)><S(x) => fibS(r)~x;
fibS(r)><Z => r~S(Z);
fibS(r)><S(x) => Dup(x1,x2)~x, add(r,r2)~r1, fib(r2)~x2, fibS(r1)~x1;


//------ ADD ------
add(r,x)><Z => r~x;
add(r,x)><S(y) => r~S(w), add(w,x)~y;


